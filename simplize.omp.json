{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#37b24d",
          "style": "plain",
          "template": "@{{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "#4263eb",
          "properties": {
            "style": "agnoster_full",
            "folder_separator_icon": "/",
            "home_icon": ""
          },
          "style": "plain",
          "template": "{{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#ae3ec9",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": " {{if .HEAD}} <#DDB15F>git:</>{{ .HEAD }}<#DDB15F></> > {{else}} > {{end}}",
          "type": "git"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}